{
  "name": "soluce",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev:en": "cross-env ENV=development ng serve --configuration=en-EN",
    "start:dev:fr": "cross-env ENV=development ng serve --configuration=fr",
    "start:dev": "cross-env ENV=development ng serve",
    "start:prod": "cross-env ENV=prod ng serve --configuration=production",
    "start:test": "cross-env ENV=test ng serve",
    "start:en": "ng serve --configuration=en-EN",
    "start:fr": "ng serve --configuration=fr",
    "build": "ng build",
    "build:en": "ng build --configuration=production,en-EN",
    "build:fr": "ng build --configuration=production,fr",
    "build:all": "ng build --configuration=production,en-EN && ng build --configuration=production,fr",
    "watch": "ng build --watch --configuration development",
    "extract-i18n": "ng extract-i18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~19.0.0",
    "@angular/common": "~19.0.0",
    "@angular/compiler": "~19.0.0",
    "@angular/core": "~19.0.0",
    "@angular/forms": "~19.0.0",
    "@angular/localize": "~19.0.6",
    "@angular/platform-browser": "~19.0.0",
    "@angular/platform-browser-dynamic": "~19.0.0",
    "@angular/router": "~19.0.0",
    "@ng-bootstrap/ng-bootstrap": "~18.0.0",
    "@popperjs/core": "~2.11.8",
    "bootstrap": "~5.3.2",
    "bootstrap-icons": "~1.11.2",
    "date-fns": "^4.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^19.0.1",
    "@angular-devkit/build-angular": "~19.0.6",
    "@angular/cli": "~19.0.6",
    "@angular/compiler-cli": "~19.0.0",
    "@types/node": "^25.0.3",
    "cross-env": "^10.1.0",
    "dotenv-webpack": "^8.1.1",
    "imports-loader": "^5.0.0",
    "typescript": "~5.6.2"
  }
}
